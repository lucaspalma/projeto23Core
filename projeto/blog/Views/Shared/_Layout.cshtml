<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width" />
<title>Resenha Cultural</title>
<link href="~/lib/jquery-ui/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" type="text/css"/>
<link href="~/css/estilo.css" rel="stylesheet" type="text/css"/>
</head>
<body>
    <!-- cabeçalho -->
    @Html.Partial("Cabecalho")
    <div class="container clearfix">
        <!-- região principal -->
        <main>
            @RenderBody()
        </main>
        <!-- menu lateral -->
        @Html.Partial("Lateral")
    </div>
    <!-- rodapé -->
    @Html.Partial("Rodape")
    <!-- seção opcional para carregar os scripts -->
    @RenderSection("Scripts", false)
</body>
</html>